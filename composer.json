{
  "name": "helixdigital-io/netlient-ugyfelkartya",
  "description": "A PHP package for the Ugyfelkartya API from Netlient.",
  "type": "library",
  "authors": [
    {
      "name": "Mark Magyar",
      "email": "mark.magyar@helixdigital.io"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^7.8",
    "nesbot/carbon": "^2.71",
    "symfony/var-dumper": "v5.4.29",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "2.28.3",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "HelixdigitalIo\\NetlientUgyfelkartya\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "scripts": {
    "post-autoload-dump": "composer normalize",
    "pint": "@php pint"
  }
}
